<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane fx:id="orderPane" minHeight="50.0" prefHeight="50.0" prefWidth="897.0" xmlns="http://javafx.com/javafx/8.0.51" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.gui.supplier.SupllierOrderPane">
   <children>
      <Pane fx:id="resasonOfDeclinePane" minHeight="92.0" prefHeight="100.0" prefWidth="897.0">
         <children>
            <Button fx:id="btnSubmit" layoutX="783.0" layoutY="62.0" mnemonicParsing="false" onAction="#onSubmit" prefHeight="25.0" prefWidth="107.0" text="Submit" />
            <Text fx:id="txtReasonOfDecline" fill="WHITE" layoutX="10.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Reason of decline :">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Text>
            <TextField fx:id="tfReasonOfDecline" layoutX="179.0" layoutY="62.0" prefHeight="25.0" prefWidth="587.0" />
         </children>
      </Pane>
      <Pane fx:id="paneOrder" minHeight="50.0" prefHeight="55.0" prefWidth="897.0">
         <children>
            <Text fx:id="txtOrderId" fill="WHITE" layoutX="138.0" layoutY="33.0" strokeType="OUTSIDE" strokeWidth="0.0" text="OrderId">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Text>
            <Text fx:id="txtStationId" fill="WHITE" layoutX="233.0" layoutY="33.0" strokeType="OUTSIDE" strokeWidth="0.0" text="StationId">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Text>
            <Text fx:id="txtFuelType" fill="WHITE" layoutX="321.0" layoutY="33.0" strokeType="OUTSIDE" strokeWidth="0.0" text="FuelType">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Text>
            <Text fx:id="txtAmount" fill="WHITE" layoutX="449.0" layoutY="33.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Amount">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Text>
            <Text fx:id="TxtTotalPrice" fill="WHITE" layoutX="533.0" layoutY="33.0" strokeType="OUTSIDE" strokeWidth="0.0" text="TotalPrice">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Text>
            <Button fx:id="btnApprove" layoutX="735.0" layoutY="13.0" mnemonicParsing="false" onAction="#onApprove" prefHeight="25.0" prefWidth="78.0" text="Supply" />
            <Button fx:id="btnDecline" layoutX="819.0" layoutY="13.0" mnemonicParsing="false" onAction="#onDecline" prefHeight="25.0" prefWidth="71.0" text="Deny" />
            <Text fx:id="txtStatus" fill="WHITE" layoutX="652.0" layoutY="33.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Status">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Text>
            <Text fx:id="txtDate" fill="WHITE" layoutX="10.0" layoutY="33.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Order date">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Text>
         </children>
      </Pane>
   </children>
</Pane>
