<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainRequestPane" prefHeight="105.0" prefWidth="910.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.gui.ceo.SubscribePane">
   <children>
      <Pane fx:id="viewPane" prefHeight="105.0" prefWidth="910.0">
         <children>
            <Label layoutX="25.0" layoutY="69.0" text="Reason of decline :" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <Button fx:id="btnSubmit" layoutX="781.0" layoutY="67.0" mnemonicParsing="false" onAction="#onSubmit" prefHeight="25.0" prefWidth="121.0" text="Submit" />
            <TextField fx:id="txtReason" layoutX="173.0" layoutY="67.0" prefHeight="25.0" prefWidth="582.0" />
         </children>
      </Pane>
      <Pane fx:id="requestPane" prefHeight="52.0" prefWidth="910.0">
         <children>
            <TextField fx:id="txtRequestID" layoutX="23.0" layoutY="12.0" prefHeight="25.0" prefWidth="64.0" />
            <TextField fx:id="txtCurrentDiscount" layoutX="144.0" layoutY="12.0" prefHeight="25.0" prefWidth="64.0" />
            <TextField fx:id="txtNewDiscount" layoutX="274.0" layoutY="13.0" prefHeight="25.0" prefWidth="64.0" />
            <TextField fx:id="txtSubscribeType" layoutX="392.0" layoutY="12.0" prefHeight="25.0" prefWidth="206.0" />
            <TextField fx:id="txtCreateTime" layoutX="624.0" layoutY="12.0" prefHeight="25.0" prefWidth="172.0" />
            <Button fx:id="btnDeny" layoutX="871.0" layoutY="11.0" mnemonicParsing="false" onAction="#onDeny" prefHeight="30.0" prefWidth="30.0" text="..." />
            <Button fx:id="btnApprove" layoutX="826.0" layoutY="11.0" mnemonicParsing="false" onAction="#onApprove" prefHeight="30.0" prefWidth="30.0" text="..." />
         </children>
      </Pane>
   </children>
</AnchorPane>
